#!/bin/sh

if [ -d /etc/X11/xinit/xinitrc.d ] ; then
    for f in /etc/X11/xinit/xinitrc.d/?*.sh ; do
        [ -x "$f" ] && . "$f"
    done
    unset f
fi

setwall -q -n

/usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1 &
picom --experimental-backends &
mpd &
unclutter &
parcellite -n &
safeeyes -e &
asus-fan-daemon &
nm-applet &

$STATUSBAR &
exec dwm
