#!/bin/sh

alias \
    activate="source venv/bin/activate" \
    cmatrix="unimatrix -n -s 96 -l o" \
    ls="ls --color=auto" \
    ll="ls -l" \
    grep="grep --color=auto" \
    fgrep="fgrep --color=auto" \
    egrep="egrep --color=auto" \
    dosbox="dosbox -conf \$XDG_CONFIG_HOME/dosbox/dosbox.conf" \
    ncdu="ncdu --color dark" \
    open="xdg-open" \
    vi="nvim"
