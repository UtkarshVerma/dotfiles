def_key "+"
    show_clock
def_key "="
    volume_up

def_key "j"
    scroll_down
def_key "k"
    scroll_up

def_key "ctrl-u"
    page_up
def_key "ctrl-d"
    page_down

def_key "."
    show_lyrics

def_key "n"
    next_found_item
def_key "N"
    previous_found_item

def_key "g"
  move_home

def_key "G"
  move_end

# not used but bound
def_key "J"
    move_sort_order_down
def_key "K"
    move_sort_order_up
