@import "launcher"

window {
    children: [inputbar, message, listview];
}
message {
    border: 0 0 1px 0;
    border-color: @color2;
    background-color: transparent;
    children: [textbox];
}
textbox {
    text-color: @color1;
    padding: 0 0 0.6em 0.6em;
    background-color: inherit;
}

prompt, element-icon {
    enabled: false;
}
inputbar {
    border: 0;
}
element-text {
    highlight: none;
}
