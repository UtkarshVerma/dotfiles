#!/bin/sh

[ $(cat /sys/class/net/w*/operstate 2>/dev/null) = up ] && wifiIcon="ğŸ“¶"

if [ $(cat /sys/class/net/e*/operstate 2>/dev/null) = up ]; then
	ethIcon="ğŸŒ"
else
	if [ ! $wifiIcon ]; then
		echo "â";
		exit 0;
	fi
fi

[ "$(ip tuntap show)" ] && vpnIcon="ğŸ”’" || vpnIcon=""
echo "$wifiIcon$ethIcon$vpnIcon"
